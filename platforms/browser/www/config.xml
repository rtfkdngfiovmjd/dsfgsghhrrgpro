<?xml version='1.0' encoding='utf-8'?>
<widget id="com.fluxsoftwares.adxpro" version="1.1.9" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>ADX Pro</name>
    <description>
        Sistema de Gestão Acadêmico Rede de Ensino Doctum - Aplicativo do Professor.
    </description>
    <author email="fluxsoftwares@gmail.com" href="http://www.fluxsoftwares.com.br/">
        Flux Softwares
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="www/res/img/icon.png" />
    <preference name="orientation" value="portrait" />
</widget>
